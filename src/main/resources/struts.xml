<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

    <package name="default" namespace="/" extends="struts-default">

        <action name="userAction" class="userSpringAction">
            <result name="success">/jsp/user.jsp</result>
        </action>

        <action name="userSpringAction" class="userAction">
            <result name="success">/jsp/user.jsp</result>
        </action>


        <!-- ======================Menu========================-->

        <action name="infoList"
                class="action.menu.TopoShow" >
            <result name="success">/jsp/user.jsp</result>
        </action>

        <action name="filter"
                class="action.menu.TopoShow" >
            <result name="success">/jsp/user.jsp</result>
        </action>

        <action name="listTopo"
                class="topoShow" >
            <result name="success">/jsp/topo.jsp</result>
        </action>

        <action name="rentTopo"
                class="action.menu.TopoShow" >
            <result name="success">/jsp/user.jsp</result>
        </action>

        <action name="connect"
                class="action.menu.TopoShow" >
            <result name="success">/jsp/user.jsp</result>
        </action>

        <!-- ======================Site========================-->
        <action name="listSite"
                class="siteShow" >
            <result name="success">/jsp/site.jsp</result>
        </action>


    </package>
</struts>